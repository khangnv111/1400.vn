@model List<W1400.DataAccess.DTO.Menu>

    <ul class="hidden-xs">
        @*<li><a href="@System.Configuration.ConfigurationManager.AppSettings["URL_ROOT"]tin-tuc">tất cả</a></li>*@
        @if (Model != null && Model.Count > 0)
        {
            for (int i = 0; i < Model.Count; i++)
            {
                if (Model[i].MenuID != 1 && Model[i].MenuID != 2)
                { 
                <li><a href="@System.Configuration.ConfigurationManager.AppSettings["URL_ROOT"]@Model[i].UrlRedirect">@Model[i].MenuName</a></li>
                }
            }
        }
    </ul>
